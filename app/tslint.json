{
  "extends": ["tslint:latest", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["node_modules/**/*.ts"]
  },
  "rules": {
    "quotemark": [true, "single"],
    "no-unused-variable": true,
    "object-literal-sort-keys": false,
    "jsx-no-lambda": false,
    "interface-name": [true, "never-prefix"]
  }
}
